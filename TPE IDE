#include<stdio.h>
int main (){
int i,m,x,result;
printf("enter the base :");
scanf("%d",&x);
printf("enter the exponentiel : ");
scanf("%d",&m);
 if(m<=0){
  result=1/x;
  }
   result=1;
  for(i=0;i<m;i++){
   result=result*x;
   }
   printf("the %d exponentiel %d is %d ",x,m,result);
   return 0;
   }
